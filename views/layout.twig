<!DOCTYPE html>
<!--[if lt IE 7]>      <html class="no-js lt-ie9 lt-ie8 lt-ie7"> <![endif]-->
<!--[if IE 7]>         <html class="no-js lt-ie9 lt-ie8"> <![endif]-->
<!--[if IE 8]>         <html class="no-js lt-ie9"> <![endif]-->
<!--[if gt IE 8]><!--> <html class="no-js"> <!--<![endif]-->
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <title>Connector Mirakl for HiPay</title>
        <meta name="description" content="">
        <meta name="viewport" content="width=device-width, initial-scale=1">

        <link href="{{ asset('css/bootstrap.min.css') }}" rel="stylesheet">
        <link href="{{ asset('css/paper.bootstrap.min.css') }}" rel="stylesheet">
        <link href="{{ asset('css/styles.css') }}" rel="stylesheet">
        <link href="{{ asset('components/datepicker/css/bootstrap-datepicker.css') }}" rel="stylesheet">
        <link href="{{ asset('components/tables/dataTables.bootstrap.min.css') }}" rel="stylesheet">
        <!-- Place favicon.ico and apple-touch-icon.png in the root directory -->
        <script src="{{ asset('js/jquery-1.10.2.min.js') }}"></script>
        <script src="{{ asset('js/bootstrap.min.js') }}"></script>
        <script src="{{ asset('components/datepicker/js/bootstrap-datepicker.min.js') }}"></script>
        <script src="{{ asset('components/datepicker/locales/bootstrap-datepicker.en-GB.min.js') }}"></script>
        <script src="{{ asset('components/datepicker/locales/bootstrap-datepicker.fr.min.js') }}"></script>
        <script src="{{ asset('components/tables/jquery.dataTables.min.js') }}"></script>
        <script src="{{ asset('components/tables/dataTables.bootstrap.min.js') }}"></script>
    </head>
    <body>
        <div class="navbar navbar-default navbar-fixed-top" role="navigation">
            <div class="container">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target=".navbar-collapse">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <a class="navbar-brand" href="{{ path('vendors') }}">
                        <img class="img-responsive" src="{{ asset('img/logo.png') }}" alt="HiPay Connector Mirakl" width="75" >
                    </a>
                </div>
                <div class="collapse navbar-collapse">
                    <ul class="nav navbar-nav">
                        <li{% if active == 'vendors' %} class="active"{% endif %}>
                            <a href="{{ path('vendors') }}">{{'your_vendors'|trans}}</a>
                        </li>
                        <li{% if active == 'transferts' %} class="active"{% endif %}>
                            <a href="{{ path('transferts') }}">{{'transfer.withdraw'|trans}}</a>
                        </li>
                        <li{% if active == 'logs' %} class="active"{% endif %}>
                            <a href="{{ path('logs') }}">{{'logs'|trans}}</a>
                        </li>
                        <li{% if active == 'settings' %} class="active"{% endif %}>
                            <a href="{{ path('settings') }}">{{'settings'|trans}}</a>
                        </li>
                        <li{% if active == 'logout' %} class="active"{% endif %}>
                            <a href="{{ path('logout') }}">{{'log.out'|trans}}</a>
                        </li>

                    </ul>
                   <p class="navbar-text navbar-right"> <span class="label label-info text-uppercase">Alpha version</span></p>
                </div><!--/.nav-collapse -->
            </div>
        </div>
        <div class="container">
            {% block content %}

            {% endblock %}
        </div><!-- /.container -->

        <script src="{{ asset('js/custom.js') }}"></script>
    </body>
</html>